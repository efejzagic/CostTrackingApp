name: dockercompose12009318408639964740
services:
  equipmentservice:
    build:
      context: D:\CostTrackingApp\CostTrackingApi
      dockerfile: EquipmentService/Dockerfile
    container_name: equipment-service
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__EquipmentConnection: User ID=postgres;Password=password;Server=127.0.0.1;Port=5432;Database=equipmentdb;IntegratedSecurity=true;Pooling=true;
    image: equipmentservice
    networks:
      dev: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 443
      published: "7001"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\efejz\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\efejz\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  storageservice:
    build:
      context: D:\CostTrackingApp\CostTrackingApi
      dockerfile: StorageService/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__StorageConnection: User ID=postgres;Password=password;Server=127.0.0.1;Port=5432;Database=storagedb;IntegratedSecurity=true;Pooling=true;
    image: storageservice
    networks:
      dev: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    - mode: ingress
      target: 443
      published: "7002"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\efejz\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\efejz\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
networks:
  dev:
    name: dockercompose12009318408639964740_dev
    driver: bridge